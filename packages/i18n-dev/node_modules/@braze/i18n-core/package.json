{
  "name": "@braze/i18n-core",
  "version": "15.0.33",
  "author": "Braze",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/braze-inc/i18n.git"
  },
  "homepage": "https://github.com/braze-inc/i18n#readme",
  "bugs": {
    "url": "https://github.com/braze-inc/i18n/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "./README.md"
  ],
  "scripts": {
    "lint": "eslint --max-warnings=0 --ext .js,.jsx,.ts,.tsx src",
    "lint:ci": "eslint --max-warnings=0 --ext .js,.jsx,.ts,.tsx src --quiet",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx src --fix",
    "prebuild": "rimraf dist",
    "build": "run-p build-js build-types",
    "build-js": "babel src -d dist --extensions \".ts,.tsx\" --ignore \"src/**/*.test.tsx\",\"src/**/*.test.ts\",\"src/**/*.stories.jsx\"",
    "build-types": "tsc --emitDeclarationOnly -p tsconfig.json",
    "test": "TZ=UTC jest --env=jsdom",
    "test:ci": "CI=true TZ=UTC jest --env=jsdom --coverage --bail",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@braze/i18n-scripts": "^15.0.32",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-icu": "2.0.3",
    "intl-messageformat": "9.13.0",
    "isomorphic-fetch": "3.0.0"
  },
  "devDependencies": {
    "pseudo-localization": "2.4.0"
  },
  "peerDependencies": {
    "@storybook/react": "^5.0.0 || ^6.0.0",
    "i18next": "^18.0.0 || ^19.0.0 || ^20.0.0",
    "pseudo-localization": "^2.1.1",
    "react-i18next": "^11.0.0"
  },
  "contributors": [
    "Samantha Abbott <samantha.abbott@braze.com>",
    "Gregory Beaver <gregory.beaver@braze.com>",
    "Haldun Anil <haldun.anil@braze.com>"
  ],
  "gitHead": "19c95b3b0e89a98ff1da245b1dad358a073e049a"
}
